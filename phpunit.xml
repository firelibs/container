<phpunit bootstrap="vendor/autoload.php">
    <testsuites>
        <testsuite name="FiReLibs/Container">
            <directory>./Tests</directory>
        </testsuite>
    </testsuites>
    <logging>
        <log type="coverage-html" target="./testresult/coverage.html" lowUpperBound="35" highLowerBound="70"/>
        <log type="coverage-clover" target="./testresult/coverage.xml"/>
        <log type="coverage-php" target="./testresult/coverage.php"/>
        <log type="coverage-text" target="./testresult/coverage.txt" showUncoveredFiles="false"/>
        <log type="junit" target="./testresult/junit.xml" logIncompleteSkipped="false"/>
        <log type="testdox-html" target="./testresult/testdox.html"/>
        <log type="testdox-text" target="./testresult/testdox.txt"/>
    </logging>
</phpunit>